endpoints {
  action-endpoints {
    action-endpoint (SearchWord) {
      accepted-inputs (word)
      local-endpoint (SearchWord.js)
    }

    action-endpoint (SearchWord) {
      accepted-inputs (word)
      remote-endpoint ("{remote.url}/words/search") {
        method (GET)
      }
    }
    
    action-endpoint (SearchMeaning) {
      accepted-inputs (word)
      local-endpoint (SearchMeaning.js)
    }
  }
  
}
